{
  "name": "@tbdcagro/style",
  "version": "0.0.15",
  "description": "Compact CSS framework",
  "author": {
    "name": "TBDC",
    "email": "engineering@tbdc.com.br"
  },
  "main": "./scss/index.scss",
  "style": "./css/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbdcagro/style.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "exports": {
    "./css": {
      "import": "./css/index.css",
      "require": "./css/index.css"
    },
    "./scss": {
      "import": "./scss/index.scss",
      "require": "./scss/index.scss"
    },
    "./scss/no-reset": {
      "import": "./scss/no-reset.scss",
      "require": "./scss/no-reset.scss"
    },
    "./scss/utils": {
      "import": "./scss/utils.scss",
      "require": "./scss/utils.scss"
    }
  },
  "files": [
    "css",
    "scss"
  ],
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework"
  ],
  "scripts": {
    "build": "rimraf css && sass ./scss:./css --no-source-map --style=compressed"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "sass": "^1.86.3"
  }
}
